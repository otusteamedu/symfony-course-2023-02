{% extends 'layout.twig' %}

{% import 'macros.twig' as macros %}

{% block title %}
    User table {{ parent() }}
{% endblock %}
{% block body %}
    <ul id="user.list" class="list-group">
        {% for user in users %}
            {{ macros.renderUser(user) }}
        {% endfor %}
    </ul>
{% endblock %}
